<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE sqlMap PUBLIC '-//ibatis.apache.org//DTD Sql Map 2.0//EN' 'http://ibatis.apache.org/dtd/sql-map-2.dtd'>

<sqlMap>
	<typeAlias alias="fundVo" type="gw.fund.FundVO" />
	
	<select id="fundList" resultClass="fundVo">
		select * from fund
	</select>
	
	<select id="fundAdd" resultClass="fundVo">
		select * from fund
	</select>
	
	<insert id="fundAddPro" parameterClass="fundVo">
		insert into fund(fund_code,fund_division,fund_days,fund_item,fund_briefs,
		fund_account,fund_deposit,fund_withdraw,fund_balance)
		values(fund_code_seq.nextval,#fund_division#,#fund_days#,
		#fund_item#,#fund_briefs#,#fund_account#,#fund_deposit#,#fund_withdraw#,#fund_balance#)
	</insert>
	
	<delete id="fundDeletePro" parameterClass="String">
		delete from fund where fund_code=#fund_code#
	</delete>
	
	<select id="fundSelectUpdate" resultClass="fundVo" parameterClass="String">
		select * from fund where fund_code=#fund_code#
	</select>
	
	<update id="fundUpdate" parameterClass="fundVo">
		update fund set fund_code=#fund_code#,fund_division=#fund_division#,fund_days=#fund_days#,
		fund_item=#fund_item#,fund_briefs=#fund_briefs#,fund_account=#fund_account#,fund_deposit=#fund_deposit#,
		fund_withdraw=#fund_withdraw#,fund_balance=#fund_balance#
	</update>
	
	<select id="fundSelectDays" resultClass="fundVo" parameterClass="java.util.HashMap">
		<![CDATA[select * from fund where fund_days >= #fund_days# and fund_days <= #fund_day#]]>
	</select>
</sqlMap>